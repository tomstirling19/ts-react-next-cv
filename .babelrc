{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./"
                ],
                "alias": {
                    "@/src": "./src",
                    "@/public": "./public"
                }
            }
        ],
        [
            "styled-components",
            {
                "ssr": true
            }
        ]
    ]
}